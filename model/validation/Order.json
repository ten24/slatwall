{
	"contexts":["save","edit","delete","addOrderItem","addOrderPayment","addPromotionCode","placeOnHold","placeOrder","takeOffHold","cancelOrder","closeOrder","createReturn"],
	"properties":{
		"orderType":		[{"contexts":"save","required":true}],
		"orderStatusType":	[{"contexts":"save","required":true}],
		"statusCode":		[
			{"contexts":"placeOrder","inList":"ostNotPlaced"},
			{"contexts":"addOrderItem","inList":"ostNotPlaced,ostNew,ostProcessing,ostOnHold"},
			{"contexts":"addOrderPayment,cancelOrder,closeOrder","inList":"ostNew,ostProcessing,ostOnHold"},
			{"contexts":"createReturn","inList":"ostNew,ostProcessing,ostOnHold,ostClosed"},
			{"contexts":"takeOffHold","inList":"ostOnHold"},
			{"contexts":"placeOnHold","inList":"ostNew,ostProcessing"}
		],
		"quantityDelivered":[
			{"contexts":"cancelOrder","maxValue":0},
			{"contexts":"createReturn","minValue":1}   
		],
		"quantityReceived":	[{"contexts":"cancelOrder","maxValue":0}],
		"total":			[{"contexts":"placeOrder","eqProperty":"paymentAmountTotal"}]
	}
}