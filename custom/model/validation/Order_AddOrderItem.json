{	
	"conditions": {
		"renewalFeeFlag": {
			"sku.product.productType.systemCode": { "eq": "RenewalFee-MP" }
		},
		"isMarketPartnerSalesOrder":{
			"account.accountType":{
				"eq":"marketPartner"
			},
			"order.orderType.systemCode":{
				"eq":"otSalesOrder"
			}
		}
	},
	"properties": {
		"isPurchasableItemFlag" :	[{ "eq": true }],
		"sku":						[{"method": "orderMinimumDaysToRenewMPFailed"}],
		"price": [ 
			{"required": true, "dataType": "numeric" }, 
			{ "conditions":"isMarketPartnerSalesOrder", "eq": true, method:"marketPartnerValidationMaxOrderAmount" }
		],
		"quantity":[
			{	
				"conditions": "renewalFeeFlag",
				"dataType": "numeric",
				"minValue": 1,
				"maxValue": 1
			}
		]
	}
}
