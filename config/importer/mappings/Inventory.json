{
    "entityName": "Inventory",

    "properties": {

        "remoteInventoryID": {
            "propertyIdentifier": "remoteID",
            "validations": {
                "required": true,
                "dataType": "string"
            }
        },
        "quantityIn": {
            "propertyIdentifier": "quantityIn",
            "validations": {
                "dataType": "numeric"
            }
        },
        "quantityOut": {
            "propertyIdentifier": "quantityOut", 
            "validations": {
                "dataType": "numeric"
            }
        },
        "cost": {
            "propertyIdentifier": "cost",
            "validations": {
                "dataType": "numeric"
            }
        },
        "cogs": {
            "propertyIdentifier": "cogs",
            "validations": {
                "dataType": "numeric"
            }
        },
        "landedCost": {
            "propertyIdentifier": "landedCost",
            "validations": {
                "dataType": "numeric"
            }
        },
        "landedAmount": {
            "propertyIdentifier": "landedAmount",
            "validations": {
                "dataType": "numeric"
            }
        },
        "currencyCode": {
            "propertyIdentifier": "currencyCode",
            "validations": {
                "dataType": "string"
            }
        }
    },


    "relations" : [
        {
            "type": "manyToOne",
            "entityName": "Stock",
            "propertyIdentifier": "stock",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
            "generatorFunction": "generateInventoryStock"
        }
        
    ],

    "dependencies" : [
        
        {
            "key"                : "remoteSkuID",
            "entityName"         : "Sku",
            "lookupKey"          : "remoteID",
            "propertyIdentifier" : "sku"
        }, 
        {
            "key"                : "remoteLocationID",
            "entityName"         : "Location",
            "lookupKey"          : "remoteID",
            "propertyIdentifier" : "location"
        }
    ],
    
    "importIdentifier": {
        "propertyIdentifier": "importRemoteID",
        "type": "composite",
        "keys": [
            "remoteInventoryID"
        ]
    }
}