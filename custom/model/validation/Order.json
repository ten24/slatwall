{
	"conditions":{
		"mpEnrollmentOrder":{
			"monatOrderType.typeCode":{
				"eq":"motMpEnrollment"
			}
		},
		"anyReturnOrder":{
			"orderType.systemCode":{
				"in":"otRefundOrder,otReturnOrder"
			}
		},
		"isMarketPartnerSalesOrder":{
			"account.accountType":{
				"eq":"marketPartner"
			},
			"orderType.systemCode":{
				"eq":"otSalesOrder"
			}
		}
	},
	"properties": {
		"orderCreatedSite": [{"contexts":"save","method":"orderCreatedSiteMatchesAccountCreatedSite"},
				{ "conditions":"isMarketPartnerSalesOrder","contexts": "save", "method": "MarketPartnerValidationMaxOrdersPlaced" },
				{ "conditions":"isMarketPartnerSalesOrder","contexts": "save", "method": "MarketPartnerValidationMaxProductPacksPurchased" }],
		"orderStatusType": [ { "contexts": "edit", "method": "isNotClosed" }],
		"orderItems":[{"conditions":"mpEnrollmentOrder","contexts":"placeOrder","method":"hasProductPackOrderItem"}],
		"subTotal": [{"contexts":"placeOrder","conditions":"anyReturnOrder","method":"subtotalWithinAllowedPercentage"}],
		
		"isLockedInProcessingFlag": [{ "contexts": "edit,clear,delete,addOrderPayment,removeOrderPayment,retryPayment,placeOrder,addOrderItem,removeOrderItem,addOrderItemGiftRecipient,addSaleOrderItem,addPromotionCode,removePromotionCode,changeCurrencyCode,updateOrderAmounts,forceItemQuantityUpdate,updateStatus,updateOrderFulfillment,removePersonalInfo,closeOrder,createReturn,takeOffHold,placeOnHold,auditRollback,releaseCredits,reopenOrder", "eq": false }],
		"statusCode":  [
			{ "contexts": "placeInProcessingOne", "inList": "ostNew,ostProcessing" },
		],
		
		"isLockedInProcessingOneFlag": [
			{ "contexts": "placeInProcessingOne", "eq": false },
			{ "contexts": "placeInProcessingTwo", "eq": true }
		],
		
		"isLockedInProcessingTwoFlag": [
			{ "contexts": "cancelOrder", "eq": false },
		]
	}
}
