{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/?slatAction=main.login&fw1pk=1"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddress"
      },
      "text": "qa@mailinator.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "QaPa33word"
    },
    {
      "type":"waitForElementPresent",
      "locator":{
        "type":"xpath",
        "value":"//*[@id="j-mobile-nav"]/ul/li[2]/ul/li[3]/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='j-login-wrapper']//button[.='Login']"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='j-mobile-nav']/div/ul/li[2]/a/i"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Logout"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Login"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
