{
    "entityName": "Order",
    
    "properties": {
        
        "remoteOrderID": {
            "propertyIdentifier": "remoteID",
            "validations": {
                "required": true,
                "dataType": "string"
            }
        },
        "orderNumber": {
            "propertyIdentifier": "orderNumber",
            "validations": {
                "required": true,
                "dataType": "numeric"
            }
        },
        "currencyCode": {
            "propertyIdentifier": "currencyCode",
            "defaultValue": "USD",
            "validations": {
                "dataType": "string"
            }
        },
        "orderOpenDateTime": {
            "propertyIdentifier": "orderOpenDateTime",
            "validations": {
                "dataType": "date"
            }
        },
        "orderIPAddress": {
            "propertyIdentifier": "orderOpenIPAddress",
            "validations": {
                "dataType": "string"
            }
        },
        "orderCloseDateTime": {
            "propertyIdentifier": "orderCloseDateTime",
            "validations": {
                "dataType": "date"
            }
        },
        "estimatedDeliveryDateTime": {
            "propertyIdentifier": "estimatedDeliveryDateTime",
            "validations": {
                "dataType": "date"
            }
        },
        "canceledDateTime": {
            "propertyIdentifier": "orderCanceledDateTime",
            "validations": {
                "dataType": "date"
            }
        },
        "orderNotes": {
            "propertyIdentifier": "orderNotes",
            "validations": {
                "dataType": "string"
            }
        }
        
    },
    
    "relations": [
        {
            "type": "manyToOne",
            "entityName": "Address",
            "propertyIdentifier": "billingAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
            "generatorFunction": "generateOrderBillingAddress"
        },
        {
            "type": "manyToOne",
            "entityName": "Address",
            "propertyIdentifier": "shippingAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
            "generatorFunction": "generateOrderShippingAddress"
        },
        {
            "type": "manyToOne",
            "entityName": "AccountAddress",
            "propertyIdentifier": "billingAccountAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
            "generatorFunction": "generateOrderBillingAccountAddress"
        },
        {
            "type": "manyToOne",
            "entityName": "AccountAddress",
            "propertyIdentifier": "shippingAccountAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
            "generatorFunction": "generateOrderShippingAccountAddress"
        }
    ],
    
     "dependencies" : [
        {
            "key"                : "remoteAccountID",
            "entityName"         : "Account",
            "lookupKey"          : "remoteID",
            "propertyIdentifier" : "account"
        },
        {
            "key": "orderTypeCode",
            "OPTIONAL_DEPENDENCY": true,
            "lookupKey": "systemCode",
            "entityName": "Type",
            "propertyIdentifier": "orderType"
        },
        {
            "key": "orderStatusTypeCode",
            "OPTIONAL_DEPENDENCY": true,
            "lookupKey": "systemCode",
            "entityName": "Type",
            "propertyIdentifier": "orderStatusType"
        },
        {
            "key": "orderOriginCode",
            "OPTIONAL_DEPENDENCY": true,
            "lookupKey": "remoteID",
            "entityName": "OrderOrigin",
            "propertyIdentifier": "orderOrigin"
        },
        {
            "key": "orderSiteCode",
            "OPTIONAL_DEPENDENCY": true,
            "lookupKey": "remoteID",
            "entityName": "Site",
            "propertyIdentifier": "orderCreatedSite"
        },
    ],
    
    "importIdentifier": {
        "propertyIdentifier": "importRemoteID",
        "type": "composite",
        "keys": [
            "remoteOrderID"
        ]
    }
}
