  "scripts": {
    "postbuild": "bower install",
    "prestart": "npm install",
    "prebuildHibachi": "npm install",
    "prebuildSlatwall": "npm install",
    "start": "http-server -a localhost -p 8000",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\"",
    "buildSlatwall": "cd admin/client && webpack",
    "buildSlatwall:prod": "cd admin/client && webpack -p --config webpack-production.config.js",
    "buildHibachi": "cd org/Hibachi/client && webpack",
    "buildHibachi:prod": "cd org/Hibachi/client && webpack --config webpack-production.config.js",
    "buildFrontend": "cd org/Hibachi/client && webpack --config webpack-frontend-develop.config.js",
    "buildFrontend:prod": "cd org/Hibachi/client && webpack --config webpack-frontend-production.config.js",
    "buildSpec": "cd admin/client && webpack --config webpack-test.config.js",
    "buildCSS": "lessc admin/client/css/main.less admin/client/css/main.css",
    "buildCustomAdmin": "node custom/client/customAdmin.js",
    "buildCustomAdmin:prod": "node custom/client/customAdminProd.js",
    "buildCustomFrontend": "custom/client/customFrontend.js",
    "buildCustomFrontend:prod": "custom/client/customFrontendProd.js",
    "buildAll": "webpack --progress ",
    "buildAll:prod": "webpack -p --progress --config webpack.config.js",
    "lint": "tsc --pretty"
  },
  "jshintConfig": {
    "predef": [
      "angular"
    ]
  }
}
