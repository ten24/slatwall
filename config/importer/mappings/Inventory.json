{
    "entityName": "Inventory",

    "properties": {

        "remoteInventoryID": {
            "propertyIdentifier": "remoteID",
            "validations": {
                "required": true,
                "dataType": "string"
            }
        },
        "quantityIn": {
            "propertyIdentifier": "quantityIn",
            "validations": {
                "dataType": "numeric"
            }
        },
        "quantityOut": {
            "propertyIdentifier": "quantityOut", 
            "validations": {
                "dataType": "numeric"
            }
        },
        "cost": {
            "propertyIdentifier": "cost",
            "validations": {
                "dataType": "numeric"
            }
        },
        "cogs": {
            "propertyIdentifier": "cogs",
            "validations": {
                "dataType": "numeric"
            }
        },
        "landedCost": {
            "propertyIdentifier": "landedCost",
            "validations": {
                "dataType": "numeric"
            }
        },
        "landedAmount": {
            "propertyIdentifier": "landedAmount",
            "validations": {
                "dataType": "numeric"
            }
        },
        "currencyCode": {
            "propertyIdentifier": "currencyCode",
            "validations": {
                "dataType": "string"
            }
        },
        "createdDateTime": {
            "propertyIdentifier": "createdDateTime",
            "validations": {
                "dataType": "date"
            }
        }

    },


    "relaions" : [
        {
            "type": "many-to-one",
            "entityName": "Stock",
            "propertyIdentifier": "stock",
            "generatorFunction": "generateInventoryStock"
        },
        {
            "type": "oneToMany",
            "entityName": "Location",
            "propertyIdentifier": "location",
            "generatorFunction": "generateInventoryLocation"
        }
        
    ],

    "dependancies" : [
        
        {
            "key"                : "remoteSkuID",
            "entityName"         : "Sku",
            "lookupKey"          : "remoteID",
            "propertyIdentifier" : "sku"
        }, 
        {
            "key"                : "remoteLocationID",
            "entityName"         : "Location",
            "lookupKey"          : "remoteID",
            "propertyIdentifier" : "location"
        }
    ],
    
    "importIdentifier": {
        "propertyIdentifier": "importRemoteID",
        "type": "composite",
        "keys": [
            "remoteInventoryID"
        ]
    }
}