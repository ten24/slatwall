{
	"conditions":{
		"mpEnrollmentOrder":{
			"monatOrderType.typeCode":{
				"eq":"motMpEnrollment"
			}
		},
		"anyReturnOrder":{
			"orderType.systemCode":{
				"in":"otRefundOrder,otReturnOrder"
			}
		},
		"isMarketPartnerSalesOrder":{
			"account.accountType":{
				"eq":"marketPartner"
			},
			"orderType.systemCode":{
				"eq":"otSalesOrder"
			},
			"orderStatusType.systemCode":{
				"eq":"ostNotPlaced"
			}
		},
		"orderIsNotInProcessing":{
			"orderStatusType.systemCode":{
				"neq":"ostProcessing"
			}
		},
		"orderIsInProcessing":{
			"orderStatusType.systemCode":{
				"eq":"ostProcessing"
			}
		}
	},
	"properties": {
		"orderCreatedSite": [{"contexts":"save","method":"orderCreatedSiteMatchesAccountCreatedSite"},
				{ "conditions":"isMarketPartnerSalesOrder","contexts": "save", "method": "MarketPartnerValidationMaxOrdersPlaced" },
				{ "conditions":"isMarketPartnerSalesOrder","contexts": "save", "method": "MarketPartnerValidationMaxProductPacksPurchased" }
			],
		"orderStatusType": [ { "contexts": "edit", "method": "isNotClosed" }],
		"orderItems":[{"conditions":"mpEnrollmentOrder","contexts":"placeOrder","method":"hasProductPackOrderItem"}],
		"subTotal": [{"contexts":"placeOrder","conditions":"anyReturnOrder","method":"subtotalWithinAllowedPercentage"}],
		
		"isLockedInProcessingFlag": [{ "contexts": "edit,clear,delete,addOrderPayment,removeOrderPayment,retryPayment,placeOrder,addOrderItem,removeOrderItem,addOrderItemGiftRecipient,addSaleOrderItem,addPromotionCode,removePromotionCode,changeCurrencyCode,forceItemQuantityUpdate,removePersonalInfo,closeOrder,createReturn,takeOffHold,placeOnHold,auditRollback,releaseCredits,reopenOrder", "eq": false }],
		"statusCode":  [
			{ "contexts": "placeInProcessingOne", "conditions": "orderIsNotInProcessing", "eq": "ostNew" }
		],
		"orderStatusType.typeCode":  [
			{ "contexts": "placeInProcessingOne", "conditions": "orderIsInProcessing", "inList": "2,processing2" }
		],
		
		"isLockedInProcessingOneFlag": [
			{ "contexts": "placeInProcessingOne", "eq": false },
			{ "contexts": "placeInProcessingTwo", "eq": true },
			{ "contexts": "updateOrderAmounts,updateStatus", "eq": false}
		]
	}
}
