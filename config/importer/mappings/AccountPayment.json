{
    "entityName": "AccountPayment",


    "properties": {

        "remoteAccountPaymentID": {
            "propertyIdentifier": "remoteID",
            "validations": {
                "required": true,
                "dataType": "string"
            }
        },
        "addressNickName": {
            "propertyIdentifier": "accountAddressName",
            "validations": {
                "dataType": "string"
            }
        }
    },

    "relations": [
        {
            "type": "manyToOne",
            "entityName": "Account",
            "propertyIdentifier": "accountID",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true
        },
        {
            "type": "manyToOne",
            "entityName": "AccountAddress",
            "propertyIdentifier": "billingAccountAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
        },
        {
             "type": "manyToOne",
            "entityName": "Address",
            "propertyIdentifier": "billingAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
        }
    ],

    "importIdentifier": {
        "propertyIdentifier": "importRemoteID",
        "type": "composite",
        "keys": [
            "remoteAccountPaymentID"
        ]
    }
}