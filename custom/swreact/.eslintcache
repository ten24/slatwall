ops = state => {\n  return {\n    auth: state.authReducer,\n    user: state.userReducer,\n  }\n}\n\nconst mapDispatchToProps = dispatch => {\n  return {\n    getUser: async () => dispatch(getUser()),\n  }\n}\nexport default connect(mapStateToProps, mapDispatchToProps)(MyAccount)\n","/home/ec2-user/environment/dev-ops/projects/StoneAndBerg/custom/swreact/src/pages/ProductSearch/ProductSearch.js",[],["226","227"],"/home/ec2-user/environment/dev-ops/projects/StoneAndBerg/custom/swreact/src/pages/CategoryListing/CategoryListing.js",[],"/home/ec2-user/environment/dev-ops/projects/StoneAndBerg/custom/swreact/src/pages/Contact/Contact.js",[],"/home/ec2-user/environment/dev-ops/projects/StoneAndBerg/custom/swreact/src/pages/Testing/Testing.js",[],"/home/ec2-user/environment/dev-ops/projects/StoneAndBerg/custom/swreact/src/pages/About/About.js",[],"/home/ec2-user/environment/dev-ops/projects/StoneAndBerg/custom/swreact/src/pages/ProductDetail/ProductDetail.js",["228","229","230","231","232","233","234","235","236"],"import React from 'react'\n// import PropTypes from 'prop-types'\nimport { Layout } from '../../components'\nimport { connect } from 'react-redux'\n\nconst ProductPageHeader = () => {\n  return (\n    <div className=\"page-title-overlap bg-lightgray pt-4\">\n      <div className=\"container d-lg-flex justify-content-between py-2 py-lg-3\">\n        <div className=\"order-lg-2 mb-3 mb-lg-0 pt-lg-2\">\n          <nav aria-label=\"breadcrumb\">\n            <ol className=\"breadcrumb text-small bg-lightgray flex-lg-nowrap justify-content-center justify-content-lg-start\">\n              <li className=\"breadcrumb-item\">\n                <a className=\"text-nowrap\" href=\"index.html\">\n                  <i className=\"fa fa-home\"></i>Home\n                </a>\n              </li>\n              <li className=\"breadcrumb-item text-nowrap\">\n                <a href=\"#\">Shop</a>\n              </li>\n              <li className=\"breadcrumb-item text-nowrap act