{
    "entityName": "AccountPaymentMethod",


    "properties": {

        "remoteAccountPaymentMethodID": {
            "propertyIdentifier": "remoteID",
            "validations": {
                "required": true,
                "dataType": "string"
            }
        },
        "isActive": {
            "propertyIdentifier": "activeFlag",
            "validations": {
                "dataType": "boolean"
            }
        },
        "currencyCode": {
            "propertyIdentifier": "currencyCode",
            "validations": {
                "dataType": "string"
            }
        },
        "ccNumberLastFour": {
            "propertyIdentifier": "creditCardLastFour",
            "validations": {
                "dataType": "string"
            }
        },
        "ccType": {
            "propertyIdentifier": "creditCardType",
            "validations": {
                "dataType": "string"
            }
        },
        "ccExpirationMonth": {
            "propertyIdentifier": "expirationMonth",
            "validations": {
                "dataType": "string"
            }
        },
        "ccExpirationYear": {
            "propertyIdentifier": "expirationYear",
            "validations": {
                "dataType": "string"
            }
        },
        "giftCardNumberEncrypted": {
            "propertyIdentifier": "giftCardNumberEncrypted",
            "validations": {
                "dataType": "string"
            }
        },
        "ccHolderName": {
            "propertyIdentifier": "nameOnCreditCard",
            "validations": {
                "dataType": "string"
            }
        }
    },

    "relations": [
        {
            "type": "manyToOne",
            "entityName": "AccountAddress",
            "propertyIdentifier": "billingAccountAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
        },
        {
             "type": "manyToOne",
            "entityName": "Address",
            "propertyIdentifier": "billingAddress",
            "excludeFromCSVTemplate" : true,
            "excludeFromValidation"  : true,
        }
    ],
    
    "dependencies" : [
        {
            "key"                : "remoteAccountID",
            "entityName"         : "Account",
            "lookupKey"          : "remoteID",
            "propertyIdentifier" : "account"
        },
    ],

    "importIdentifier": {
        "propertyIdentifier": "importRemoteID",
        "type": "composite",
        "keys": [
            "remoteAccountPaymentMethodID"
        ]
    }
}