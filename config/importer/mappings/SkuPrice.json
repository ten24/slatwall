{
    "entityName": "SkuPrice",
    
    "properties": {
        
        "remoteSkuPriceID": {
            "propertyIdentifier": "remoteID",
            "validations": {
                "dataType": "string"
            }
        },
        "minQuantity":  {
            "propertyIdentifier": "minQuantity",
            "validations": {
                "dataType": "numeric"
            }
        },
        "maxQuantity": {
            "propertyIdentifier": "maxQuantity",
            "validations": {
                "dataType": "numeric"
            }
        },
        "currencyCode": {
            "propertyIdentifier": "currencyCode",
            "defaultValue": "USD",
            "validations": {
                "dataType": "string"
            }
        },
        "price": {
            "propertyIdentifier": "price",
            "validations": {
                "required": true,
                "dataType": "numeric"
            }
        },
        "listPrice": {
            "propertyIdentifier": "listPrice",
            "validations": {
                "required": true,
                "dataType": "numeric"
            }
        },
        "skuPriceActiveFlag": {
            "propertyIdentifier": "activeFlag",
            "defaultValue": true,
            "validations": {
                "dataType": "boolean"
            }
        }
    },
    
    "relations" : [
        {
            "type": "manyToOne",
            "entityName": "PriceGroup",
            "propertyIdentifier": "priceGroup",
            "generatorFunction": "generateSkuPricePriceGroup"
        }
    ],
    
    "importIdentifier": {
        "propertyIdentifier": "importRemoteID",
        "type": "composite",
        "keys": [
            "remoteSkuID", "remoteSkuPriceID"
        ]
    }
}