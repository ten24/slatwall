{  
    "company":{  
        "fieldType":"text"
    },
    "division":{  
        "fieldType":"text"
    },
    "department":{  
        "fieldType":"text"
    },
    "originCity":{  
        "fieldType":"text"
    },
    "originMainDivision":{  
        "fieldType":"text"
    },
    "originPostalCode":{  
        "fieldType":"text"
    },
