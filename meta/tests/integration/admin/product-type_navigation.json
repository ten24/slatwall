{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/?slatAction=main.login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddress"
      },
      "text": "qa@mailinator.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "QaPa33word"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='j-login-wrapper']//button[.='Login']"
      }
    },
    {
      "type": "waitForEval",
      "script": "return document.readyState",
      "value": "complete"
    },

    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Product Types"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Product Types"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}