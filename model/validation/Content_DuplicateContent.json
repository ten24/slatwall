{
	"properties": {
		"urlTitle": [
			{
				"contexts": "duplicateContent",
				"required": false
			},
			{
				"contexts": "duplicateContent",
				"regex": "^[A-Za-z0-9-]+$"
			}
		],
		"urlTitlePath": [
			{
				"contexts": "duplicateContent",
				"method": "isUniqueUrlTitlePathBySite"
			}
		],
		"newContent.site": [
			{
				"contexts": "duplicateContent",
				"required": true
			}
		],
		"newContent.parentContent": [
			{
				"contexts": "duplicateContent",
				"required": true
			}
		]
	}
}
